.container .start{display:flex;height:100%;align-items:center}.container .start .startbtn{width:200rpx;height:200rpx;text-align:center;font-size:36rpx;line-height:200rpx;font-weight:bold;background:radial-gradient(circle, #ff5777, #ff0077);color:#fff;border-radius:50%}.container .content{width:100%}.container .content .wrap .time{font-size:36rpx;color:#333;text-align:center}.container .content .wrap .progress{width:80%;margin:0 auto}.container .content .wrap .explain{color:#999;font-size:22rpx;text-align:center}.container .content .wrap .box{display:flex;flex-wrap:wrap;width:100%;background:#000;justify-content:space-around}.container .content .wrap .box.hide{animation:hide .5s ease-out}.container .content .wrap .box.show{animation:show .5s ease-out}.container .content .wrap .box .boxview{display:flex;justify-content:center;align-items:center;color:#666;font-size:36rpx}.container .content .wrap .over{display:flex;height:200px;flex-direction:column;align-items:center;justify-content:space-around;background-image:linear-gradient(#ff5777, #ff0077);color:white}.container .content .wrap .over.hide{animation:hide .5s ease-out}.container .content .wrap .over.show{animation:show .5s ease-out}.container .content .wrap .over .nick{font-size:32rpx}.container .content .wrap .over .grade{font-size:36rpx;padding-top:10px}.container .content .wrap .over .choose{width:100%;display:flex;justify-content:space-around;align-items:center;font-size:28rpx;padding-top:10px}.container .content .wrap .over .choose .btn{border:1px solid #ffe8ee;border-radius:5px;padding:5px}.container .content .wrap .margin-top{margin-top:15px}.container .touch-class{opacity:.7}@keyframes hide{0%{transform:rotate3d(0, 1, 0, 0deg)}100%{transform:rotate3d(0, 1, 0, 90deg)}}@keyframes show{0%{transform:rotate3d(0, 1, 0, 90deg)}100%{transform:rotate3d(0, 1, 0, 0deg)}}